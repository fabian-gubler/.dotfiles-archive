def_key "up"
  scroll_up

def_key "down"
  scroll_down

def_key "i"
  next_column

def_key "m"
  previous_column

def_key "ctrl-u"
    page_up

def_key "ctrl-d"
    page_down

def_key "enter"
  play_item

def_key "space"
  add_item_to_playlist

def_key "ctrl-p"
  volume_up

def_key "ctrl-n"
  volume_down

def_key "right"
  next_screen

def_key "left"
  previous_screen

def_key "s"
  set_volume
